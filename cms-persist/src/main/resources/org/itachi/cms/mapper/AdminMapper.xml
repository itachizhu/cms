<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.itachi.cms.mapper.AdminMapper">
    <resultMap id="adminUserResultMap" type="adminUserDTO">
        <id property="id" column="id"/>
        <result property="account" column="accout" javaType="string"/>
    </resultMap>

    <sql id="select_list">
        select * from t_admuser
    </sql>

    <select id="getAdminUser" resultMap="adminUserResultMap">
        <include refid="select_list"/>
        where id=#{id}
    </select>
</mapper>